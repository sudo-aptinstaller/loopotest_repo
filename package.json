{
  "name": "loopo",
  "version": "0.0.4",
  "description": "Client Side Handle Of ScrapeIN",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "publish": "build -p always"
  },
  "repository": "https://github.com/Automation-OnBlick/loopo",
  "keywords": [
    "Loopo",
    "Automation"
  ],
  "author": "Loopo Inc",
  "license": "GPL",
  "devDependencies": {
    "electron": "^11.1.0",
    "electron-builder": "^20.44.4"
  },
  "dependencies": {
    "asar": "^3.0.3",
    "crypto-js": "^4.0.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-log": "^1.3.0",
    "electron-packager": "^15.1.0",
    "electron-updater": "^4.3.5",
    "fs": "0.0.1-security",
    "jquery": "^3.5.1"
  },
  "build": {
    "appId": "com.github.Automation-OnBlick.loopo",
    "productName": "Loopo",
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "icon": "icons/icon.ico"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "Automation-OnBlick",
        "repo": "loopo"
      }
    ],
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    },
    "asar": false
  }
}
